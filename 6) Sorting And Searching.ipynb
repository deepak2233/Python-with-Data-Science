    "def insertionSort(arr, n) :\n",
    "    i = 1\n",
    "    while i < n :\n",
    "        temp = arr[i]\n",
    "        j = i - 1\n",
    "        while j >= 0 :\n",
    "            if arr[j] > temp :\n",
    "                arr[j + 1] = arr[j]\n",
    "            else :\n",
    "                break\n",
    "                \n",
    "            j -= 1\n",
    "            \n",
    "        arr[j + 1] = temp\n",
    "        i += 1\n",
    "        \n",
    "\n",
    "t = int(input())\n",
    "while t>0:\n",
    "    n = int(input())\n",
    "    arr = [int(i) for i in input().split()]\n",
    "    insertionSort(arr,n)\n",
    "    print(arr)\n",
    "    t-=1"
   ]
