    "def selectionSort(arr):\n",
    "    length = len(arr)\n",
    "    for i in range(length-1):\n",
    "        minIndex = i\n",
    "        for j in range(i+1,length):\n",
    "            if(arr[j] < arr[minIndex]):\n",
    "                minIndex = j\n",
    "        temp = arr[i]\n",
    "        arr[i] = arr[minIndex]\n",
    "        arr[minIndex] = temp\n",
    "        \n",
    "t = int(input())\n",
    "while t>0:\n",
    "    n = int(input())\n",
    "    arr = [int(i) for i in input().split()]\n",
    "    insertionSort(arr,n)\n",
    "    print(arr)\n",
    "    t-=1"
