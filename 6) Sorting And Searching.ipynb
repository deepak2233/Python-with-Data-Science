    "def bubbleSort(arr, n):\n",
    "    for i in range(n-1):\n",
    "        for j in range(n-i-1):\n",
    "            if arr[j]>arr[j+1]:\n",
    "                temp = arr[j]\n",
    "                arr[j] = arr[j+1]\n",
    "                arr[j+1] = temp\n",
    "\n",
    "t = int(input())\n",
    "while t>0:\n",
    "    n = int(input())\n",
    "    arr = [int(i) for i in input().split()]\n",
    "    bubbleSort(arr,n)\n",
    "    print(arr)\n",
    "    t-=1\n",
    "    "
