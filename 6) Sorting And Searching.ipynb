    "from sys import stdin\n",
    "def binarySearch(arr, n, x) :\n",
    "    start = 0;\n",
    "    end = n - 1\n",
    "    mid = start\n",
    "    while start <= end :\n",
    "        mid = start + (end - start) // 2\n",
    "        if arr[mid] > x :\n",
    "            end = mid - 1\n",
    "        elif arr[mid] < x :\n",
    "            start = mid + 1\n",
    "        else :\n",
    "            return mid\n",
    "    return -1\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "#main\n",
    "n = int(input())\n",
    "arr = [int(i) for i in input().split()]\n",
    "t = int(input()) // \n",
    "while t > 0 :\n",
    "    x = int(input().strip())\n",
    "    print(binarySearch(arr, n, x))\n",
    "    t -= 1"
