    "# Second Largest Element In Array\n",
    "MIN_VALUE = -2147483648 \n",
    "def secondLargestElement(arr, n): \n",
    "    if n == 0 : \n",
    "        return MIN_VALUE\n",
    "    largest = arr[0]\n",
    "    secondLargest = MIN_VALUE\n",
    "    for i in range(n) : \n",
    "        if largest < arr[i] :\n",
    "            secondLargest = largest\n",
    "            largest = arr[i] \n",
    "        elif secondLargest < arr[i] and arr[i] != largest :\n",
    "            secondLargest = arr[i]\n",
    "    return secondLargest\n",
